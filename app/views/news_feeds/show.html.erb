<%- model_class = @news_feed.class -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:title) %>:</strong></dt>
  <dd><%= @news_feed.title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:description) %>:</strong></dt>
  <dd><%= @news_feed.description %></dd>
  <dt><strong><%= model_class.human_attribute_name(:link) %>:</strong></dt>
  <dd><%= @news_feed.link %></dd>
  <dt><strong><%= model_class.human_attribute_name(:published_date) %>:</strong></dt>
  <dd><%= @news_feed.published_date %></dd>
  <dt><strong><%= model_class.human_attribute_name(:keywords) %>:</strong></dt>
  <dd><%= @news_feed.keywords %></dd>
  <dt><strong><%= model_class.human_attribute_name(:image_urls) %>:</strong></dt>
  <dd><%= @news_feed.image_urls %></dd>
  <dt><strong><%= model_class.human_attribute_name(:rss_id) %>:</strong></dt>
  <dd><%= @news_feed.rss_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              news_feeds_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_news_feed_path(@news_feed), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              news_feed_path(@news_feed),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
