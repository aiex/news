<table class='table table-bordered table-striped' id='listsTable'>
  <tr>
    <th>Started at</th>
    <th>Status</th>
    <th>Finished at / Failed at</th>
  </tr>
  <% crons.each do |cron| %>
    <tr>
      <td><%= cron.created_at.in_time_zone("Mumbai") %></td>
      <td><span class="label <%= cron_class(cron.status) %>"><%= cron.status %></span></td>
      <td><%= cron.finished_at.try(:in_time_zone, "Mumbai") %></td>
    </tr>
  <% end %>
</table>
<%= paginate crons %>